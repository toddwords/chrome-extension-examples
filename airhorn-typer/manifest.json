{
  "name": "AIRHONK",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Boilerplate extension for class",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.twitter.com/*"
      ],
      "js": [
        "lib/mousetrap.min.js","lib/jquery-1.11.1.min.js","inject.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "airhorn.mp3"
  ]
}