{
  "name": "Page Specific Alerts",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Makes changes to web pages",
  "permissions": [
    "tabs", "tts"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "lib/jquery-1.11.1.min.js","inject.js"
      ]
    }
  ]
}