{
  "name": "Social Media Consequences",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Try visiting Facebook, Twitter, or Reddit",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*","https://*.twitter.com/*", "*://*.reddit.com/*"
      ],
      "js": [
        "inject.js"
      ]
    }
  ]
}